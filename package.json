{
    "name": "control-center-monorepo",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "nx serve control-center --port 4200",
        "prod": "cross-env NODE_ENV=stage nx serve control-center --port 4200 --configuration=production",
        "stage": "cross-env NODE_ENV=stage nx serve control-center --port 4201 --configuration=stage",
        "build": "nx run-many -t build",
        "test": "nx test",
        "lint": "nx run-many -t lint",
        "lint:fix": "npm run lint -- --fix=true",
        "format": "nx format",
        "format:fix": "nx format:write",
        "spell": "cspell --no-progress **",
        "spell:unused": "npx cspell-check-unused-words",
        "spell:fix": "npm run spell -- **  --show-suggestions --show-context",
        "fix": "npm run lint:fix && npm run format:fix && npm run spell:fix",
        "check": "concurrently --prefix \"[{command}]\" --prefixColors auto --prefixLength 20 \"npm run format\" \"npm run lint\" \"npm run spell\""
    },
    "dependencies": {
        "@angular/animations": "20.1.2",
        "@angular/cdk": "20.1.2",
        "@angular/common": "20.1.2",
        "@angular/compiler": "20.1.2",
        "@angular/core": "20.1.2",
        "@angular/forms": "20.1.2",
        "@angular/material": "20.1.2",
        "@angular/material-date-fns-adapter": "20.1.2",
        "@angular/platform-browser": "20.1.2",
        "@angular/router": "20.1.2",
        "@ngneat/input-mask": "^6.1.0",
        "@s-libs/ng-core": "^20.0.0",
        "@tailwindcss/postcss": "^4.1.11",
        "@vality/domain-proto": "2.0.1-ff9b01f.0",
        "@vality/dominator-proto": "1.0.1-67ea35c.0",
        "@vality/fistful-proto": "2.0.1-fe195c5.0",
        "@vality/machinegun-proto": "1.0.1-cc2c27c.0",
        "@vality/magista-proto": "2.0.2-0e0b068.0",
        "@vality/repairer-proto": "2.0.2-1a48729.0",
        "@vality/scrooge-proto": "0.1.1-42aba67.0",
        "@vality/thrift-ts": "2.5.1-2b658f2.0",
        "@vality/woody": "0.1.3",
        "date-fns": "^4.1.0",
        "keycloak-angular": "^20.0.0",
        "keycloak-js": "^26.2.0",
        "lodash-es": "^4.17.21",
        "monaco-editor": "^0.52.2",
        "rxjs": "7.8.1",
        "short-uuid": "^5.2.0",
        "tailwindcss": "^4.1.11",
        "tslib": "2.3.1",
        "utility-types": "3.11.0",
        "zod": "^4.0.14",
        "zone.js": "0.15.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "20.1.2",
        "@angular-devkit/core": "20.1.2",
        "@angular-devkit/schematics": "20.1.2",
        "@angular/cli": "20.1.2",
        "@angular/compiler-cli": "20.1.2",
        "@cspell/dict-ru_ru": "^2.3.0",
        "@nx/angular": ">=20.0.0",
        "@nx/eslint-plugin": "21.3.11",
        "@nx/jest": "21.3.11",
        "@nx/js": "21.3.11",
        "@nx/node": "21.3.11",
        "@nx/workspace": "21.3.11",
        "@schematics/angular": "20.1.2",
        "@swc-node/register": "~1.9.1",
